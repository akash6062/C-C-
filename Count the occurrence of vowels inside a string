#include<stdio.h>
void main()
{
char arr[25];
int i=0,count=0;
printf("\n Enter the sring : ");
gets(arr);
while(arr[i]!='\0')
	{
      if(arr[i]=='a' || arr[i]=='e' || arr[i]=='i' || arr[i]=='o' || arr[i]=='u'|| arr[i]=='A' || arr[i]=='E' || arr[i]=='I' || arr[i]=='O' || arr[i]=='U')
		{
		count++;
		}
	i++;
	}
printf("\n Number of occurance of vowel is %d",count);
}
