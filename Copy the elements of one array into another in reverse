#include<stdio.h>
void main()
{
	int a[10], b[10], i, j;
	printf("\nEnter 10 Elements : ");				//Accepting values into Array
	for (i = 0; i<10; i++)
	{
		scanf("%d", &a[i]);
	}
	for (i = 0, j = 9; i<10; i++, j--)				//Copying data from array
	{
		b[i] = a[j];
	}
	printf("\nArray after Copying in Reverse Order : ");		//Printing of all elements of array copied	
	for (i = 0; i<10; i++)
	{
		printf("%d ", b[i]);					
	}
	}



