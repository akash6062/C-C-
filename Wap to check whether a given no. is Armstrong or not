#include<stdio.h>
	#include<conio.h>
	#include<math.h.h>
	int main()
	{
		clrscr();	
		int num, orinum, rem, result=0, n=0;
		printf("Enter the integer num.  \n");
		scanf("%d",&num);
		orinum= num;
		while(orinum != 0)
		{
			orinum/= 10;
			n++;
		}
		while
		{
			rem= orinum % 10;
			result= pow(rem, num);
			orinum /= 10;
		}
		if(result== num)
			printf("%d is an Armstrong number", num);
		else
			printf("%d is not an Armstrong number", num);
		return 0;
		getch();
	}
