	printf("Enter the Integer Value of b: \n");
	scanf("%d", &b);
	printf("Enter the Integer Value of c: \n");
	scanf("%d", &c);
	if(a>b)
	{
		if(a>c)
			printf("The entered number 'a' is maximum");
		else
			printf("The entered number 'c' is maximum);
	}
	else
	{
		if(b>c)
			printf("The entered number 'b' is maximum");
		else
			printf("The entered number 'c' is maximum);
	}
